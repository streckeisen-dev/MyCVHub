{
  "name": "my-cv-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "LGPL-3.0-or-later",
  "scripts": {
    "dev": "vite",
    "build": "yarn type-check && yarn build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest run --dom",
    "test-watch": "vitest --reporter=default",
    "test-coverage": "vitest run --coverage",
    "cypress": "node --loader ts-node/esm node_modules/.bin/cypress open",
    "cypress:results:merge": "jrm ./cypress/component-results.xml \"./cypress/results/*.xml\""
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "lodash": "^4.17.21",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.8.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.25.1",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.4",
    "vue-tsc": "^2.2.10",
    "vitest": "^3.1.2",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^17.4.6",
    "@vitest/coverage-istanbul": "^3.1.2",
    "cypress": "^14.3.2",
    "@cypress/vue": "^6.0.2",
    "@cypress/vite-dev-server": "^6.0.3",
    "ts-node": "^10.9.2",
    "junit-report-merger": "^7.0.1"
  }
}
