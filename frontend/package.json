{
  "name": "my-cv-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "LGPL-3.0-or-later",
  "scripts": {
    "dev": "vite",
    "build": "yarn type-check && yarn build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest run --dom",
    "test-watch": "vitest --reporter=default",
    "test-coverage": "vitest run --coverage",
    "cypress": "node --loader ts-node/esm node_modules/.bin/cypress open",
    "cypress:results:merge": "jrm ./cypress/component-results.xml \"./cypress/results/*.xml\""
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "lodash": "^4.17.21",
    "vite-plugin-vuetify": "^2.1.2",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.11.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "@cypress/vue": "^6.0.2",
    "@eslint/compat": "^1.4.0",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.15.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/lodash": "^4.17.21",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "happy-dom": "^19.0.2",
    "junit-report-merger": "^8.0.0",
    "prettier": "^3.7.3",
    "sass-embedded": "^1.93.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vite": "^6.3.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.5"
  },
  "packageManager": "yarn@4.10.3"
}
